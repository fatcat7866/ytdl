DATABASE_URL="file:./dev.db"

# yt-dlp binary path (default: assumes it's in PATH)
YTDLP_PATH=yt-dlp

# S3-Compatible Storage (leave empty to disable file downloads)
# For AWS S3:
#   S3_ENDPOINT=https://s3.us-east-1.amazonaws.com
#   S3_REGION=us-east-1
# For Cloudflare R2:
#   S3_ENDPOINT=https://<account-id>.r2.cloudflarestorage.com
#   S3_REGION=auto
S3_ENDPOINT=
S3_REGION=auto
S3_BUCKET=
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=
S3_PUBLIC_URL=
